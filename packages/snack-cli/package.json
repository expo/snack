    "snack-content": "3.6.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/snack.git",
    "directory": "packages/snack-cli"
  },
  "bugs": {
