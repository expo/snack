{
  "name": "snack-babel-standalone",
  "version": "2.0.0",
  "description": "Babel for React Native",
  "main": "./build/runtime.js",
  "types": "./types/runtime.d.ts",
  "typesVersions": {
    "*": {
      "eslint": [
        "./types/eslint.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./build/runtime.js",
      "types": "./types/runtime.d.ts"
    },
    "./eslint": {
      "require": "./build/eslint.js",
      "types": "./types/eslint.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/snack.git",
    "directory": "packages/snack-babel-standalone"
  },
  "bugs": {
    "url": "https://github.com/expo/snack/issues"
  },
  "author": "Expo <support@expo.dev>",
  "license": "MIT",
  "volta": {
    "node": "16.14.2"
  },
  "scripts": {
    "clean": "git clean -xdf ./build",
    "build": "webpack",
    "compile": "WEBPACK_PRODUCTION=true webpack",
    "analyze": "WEBPACK_ANALYZE=true webpack",
    "prepublishOnly": "yarn clean && yarn compile"
  },
  "files": [
    "build",
    "types"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-arrow-functions": "^7.18.6",
    "@babel/plugin-transform-async-to-generator": "^7.18.6",
    "@babel/plugin-transform-shorthand-properties": "^7.18.6",
    "@babel/plugin-transform-template-literals": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "metro-react-native-babel-preset": "^0.64.0",
    "node-polyfill-webpack-plugin": "^2.0.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0"
  }
}
