{
  "name": "snack-sdk",
  "version": "6.4.0",
  "description": "The Expo Snack SDK",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "CHANGELOG.md",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/expo/snack.git",
    "directory": "packages/snack-sdk"
  },
  "bugs": {
    "url": "https://github.com/expo/snack/issues"
  },
  "scripts": {
    "build": "tsc",
    "test": "yarn test:mock",
    "test:mock": "yarn run jest",
    "test:local": "SNACK_ENV=local yarn run jest",
    "test:staging": "SNACK_ENV=staging yarn run jest",
    "test:prod": "SNACK_ENV=production yarn run jest",
    "lint": "eslint .",
    "doc": "yarn typedoc",
    "doc:ci": "yarn doc && node ./scripts/checkUncommittedDocChanges.js"
  },
  "author": "Expo <support@expo.dev>",
  "license": "MIT",
  "volta": {
    "node": "18.17.1"
  },
  "devDependencies": {
    "@expo/spawn-async": "^1.5.0",
    "@koa/router": "^10.0.0",
    "@types/diff": "^4.0.2",
    "@types/jest": "^26.0.20",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__router": "^8.0.3",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/node-fetch": "^2.5.7",
    "@types/pubnub": "^7.2.0",
    "@types/semver": "^7.3.4",
    "@types/ua-parser-js": "^0.7.33",
    "@types/validate-npm-package-name": "^3.0.2",
    "eslint": "^8.49.0",
    "eslint-config-universe": "^12.0.0",
    "form-data": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "prettier": "^3.0.3",
    "ts-jest": "^26.5.0",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "diff": "^4.0.2",
    "fetch-ponyfill": "^7.0.0",
    "lodash": "^4.17.20",
    "nanoid": "^3.1.20",
    "nullthrows": "^1.1.1",
    "pubnub": "^7.2.0",
    "semver": "^7.3.4",
    "snack-content": "~3.4.0",
    "socket.io-client": "~4.8.1",
    "ua-parser-js": "^0.7.22",
    "validate-npm-package-name": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "universe/node",
    "ignorePatterns": [
      "example",
      "build"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": true,
          "caughtErrors": "all",
          "caughtErrorsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
