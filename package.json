{
  "name": "@expo/snack",
  "private": true,
  "author": "Expo",
  "license": "MIT",
  "volta": {
    "node": "16.14.2"
  },
  "scripts": {
    "build": "cd packages/snack-content && yarn build && cd .. && cd snack-sdk && yarn build",
    "start": "cd packages/snack-term && yarn start && cd .."
  },
  "workspaces": {
    "packages": [
      "packages/snack-content",
      "packages/snack-proxies",
      "packages/snack-sdk",
      "packages/snack-term",
      "website",
      "snackager"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chalet": "^0.9.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.12.0",
    "eslint-config-universe": "^10.0.0",
    "prettier": "^2.6.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "eslint@8.20.0": "./packages/snack-eslint-standalone/patches/eslint@8.20.0.patch",
      "eslint-plugin-react@7.30.1": "./packages/snack-eslint-standalone/patches/eslint-plugin-react@7.30.1.patch",
      "react-native@0.69.4": "./runtime/patches/react-native@0.69.4.patch",
      "react-native-web@0.18.9": "./runtime/patches/react-native-web@0.18.9.patch"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "expo-modules-*",
        "typescript"
      ]
    }
  }
}
