{
  "name": "snack",
  "version": "0.0.0",
  "description": "Playground for Expo!",
  "author": "exponent.team@gmail.com",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "owner": "exponent",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "lint": "eslint src types --ext '.js,.ts,.tsx'",
    "typescript": "tsc",
    "test": "jest",
    "deploy:staging": "EXPO_STAGING=1 expo publish --clear",
    "deploy:prod": "CLOUD_ENV=production NODE_ENV=production expo publish --clear",
    "predeploy:web:staging": "expo build:web",
    "predeploy:web:prod": "CLOUD_ENV=production NODE_ENV=production expo build:web",
    "deploy:web:staging": "s3-deploy './web-build/**' --cwd './web-build' --region us-west-1 --bucket snack-web-player-staging --filePrefix v2/$(node -pe \"require('./app.json').expo.sdkVersion.split('.')[0]\")",
    "deploy:web:prod": "s3-deploy './web-build/**' --cwd './web-build' --region us-west-1 --bucket snack-web-player --filePrefix v2/$(node -pe \"require('./app.json').expo.sdkVersion.split('.')[0]\")"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@expo/babel-standalone": "1.0.3",
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "^1.15.0",
    "diff": "^5.0.0",
    "escape-string-regexp": "^5.0.0",
    "expo": "~43.0.0-beta.3",
    "expo-analytics-amplitude": "~11.0.1",
    "expo-app-loading": "^1.1.2",
    "expo-asset": "~8.4.1",
    "expo-barcode-scanner": "~11.1.0",
    "expo-constants": "~12.1.0",
    "expo-file-system": "~13.0.1",
    "expo-font": "~10.0.1",
    "expo-keep-awake": "~10.0.0",
    "expo-status-bar": "~1.1.0",
    "expo-updates": "~0.10.2",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "pubnub": "^4.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-view-shot": "3.1.2",
    "react-native-web": "0.17.1",
    "source-map": "0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@expo/webpack-config": "^0.16.2",
    "@types/diff": "^5.0.0",
    "@types/pubnub": "^4.0.6",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "babel-preset-expo": "^8.4.1",
    "eslint": "^7.32.0",
    "eslint-config-universe": "^7.0.1",
    "jest-expo": "^42.1.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "s3-deploy": "^1.4.0",
    "typescript": "~4.3.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
