    "@babel/core": "^7.20.0",
    "@expo/spawn-async": "^1.7.2",
    "@types/diff": "^5.0.0",
    "@types/jest": "^26.0.24",
    "@types/pubnub": "^7.2.0",
    "@types/react": "~18.2.45",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "~0.70.6",
    "babel-preset-expo": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "getenv": "^1.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.1",
