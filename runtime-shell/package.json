    "expo-constants": "~17.0.2",
    "expo-crypto": "~14.0.1",
    "expo-file-system": "~18.0.2",
    "expo-font": "~13.0.1",
    "expo-keep-awake": "~14.0.1",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.2",
    "expo-splash-screen": "~0.29.6",
    "expo-status-bar": "~2.0.0",
    "expo-updates": "~0.26.5",
