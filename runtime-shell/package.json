  "author": "exponent.team@gmail.com",
  "private": true,
  "main": "index.js",
  "owner": "exponent",
  "scripts": {
    "start": "CLOUD_ENV=staging expo start",
    "web": "CLOUD_ENV=staging expo start --web",
