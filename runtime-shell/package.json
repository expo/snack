{
  "name": "snack",
  "version": "0.0.0",
  "description": "Playground for Expo!",
  "author": "exponent.team@gmail.com",
  "private": true,
  "main": "index.js",
  "owner": "exponent",
  "scripts": {
    "start": "EXPO_PUBLIC_SNACK_ENV=staging expo start",
    "web": "EXPO_PUBLIC_SNACK_ENV=staging expo start --web",
    "postinstall": "patch-package",
    "lint": "eslint .",
    "typescript": "tsc",
    "test": "jest",
    "deploy:staging": "EXPO_PUBLIC_SNACK_ENV=staging EXPO_STAGING=1 expo-cli publish --clear",
    "deploy:prod": "EXPO_PUBLIC_SNACK_ENV=production expo-cli publish --clear",
    "deploy:web:staging": "EXPO_PUBLIC_SNACK_ENV=staging node ./web/deploy-script.js",
    "deploy:web:prod": "EXPO_PUBLIC_SNACK_ENV=production node ./web/deploy-script.js"
  },
  "dependencies": {
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "assert": "^2.1.0",
    "expo": "^50.0.0-preview.8",
    "expo-analytics-amplitude": "~11.3.0",
    "expo-asset": "~9.0.2",
    "expo-barcode-scanner": "~12.9.2",
    "expo-constants": "~15.4.2",
    "expo-file-system": "~16.0.2",
    "expo-font": "~11.10.0",
    "expo-keep-awake": "~12.8.0",
    "expo-linking": "~6.2.1",
    "expo-random": "~13.6.0",
    "expo-router": "~3.4.1",
    "expo-splash-screen": "~0.26.1",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.6",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.1",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-pager-view": "6.2.2",
    "react-native-reanimated": "~3.6.0",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "~3.27.0",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.6",
    "snack-runtime": "file:../packages/snack-runtime"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/spawn-async": "^1.7.2",
    "@types/diff": "^5.0.0",
    "@types/jest": "^29.5.4",
    "@types/pubnub": "^7.2.0",
    "@types/react": "~18.2.45",
    "@types/react-dom": "~18.0.10",
    "@types/react-native": "~0.70.6",
    "babel-preset-expo": "^10.0.0",
    "eslint": "^8.49.0",
    "eslint-config-universe": "^12.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.3",
    "s3-deploy": "^1.4.0",
    "semver": "^7.5.1",
    "typescript": "^5.3.0"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "jest": {
    "preset": "jest-expo"
  }
}
