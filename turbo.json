{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "start": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["**/*.{ts,tsx,js,jsx}"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "build/**"
      ]
    },
    "snack-modules-packager#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "build/**"
      ],
      "env": [
        "APP_VERSION" // see: ./snackager/Dockerfile#L52
      ]
    },
    "@expo/web-snack#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "build/**" // TODO: validate this
      ],
      "env": [
        "API_SERVER_URL", // see: ./website/Dockerfile#L23
        "APP_VERSION", // see: ./website/Dockerfile#L25
        "DEPLOY_ENVIRONMENT", // see: ./website/Dockerfile#L27
        "IMPORT_SERVER_URL", // see: ./website/Dockerfile#L29
        "LEGACY_SERVER_URL", // see: ./website/Dockerfile#L31
        "LEGACY_SNACK_SERVER_URL", // see: ./website/Dockerfile#L33
        "NODE_ENV", // see: ./website/webpack.config.js#L78
        "RUDDERSTACK_DATA_PLANE_URL", // see: ./website/Dockerfile#L35
        "RUDDERSTACK_WRITE_KEY", // see: ./website/Dockerfile#L37
        "SERVER_URL", // see: ./website/Dockerfile#L39
        "SNACK_AMPLITUDE_KEY", // see: ./website/Dockerfile#L41
        "SNACK_SERVER_URL", // see: ./website/Dockerfile#L43
        "SNACK_WEBPLAYER_URL", // see: ./website/Dockerfile#L45
        "SNACKPUB_URL", // see: ./website/Dockerfile#L47
        "SNACK_RUNTIME_ENDPOINT" // see: ./website/Dockerfile#L49
      ]
    }
  }
}
